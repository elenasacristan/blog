<%- include('partials/header') -%>
<h1>Compose</h1>
<form action="/compose" method="post">

    <div class="form-group">
        <label for="titlePost">Title</label>
        <input class="form-control" id="titlePost" type="text" name="titlePost">
    </div>
    <div class="form-group">
        <label for="description">Post</label>
        <textarea name="bodyPost" class="form-control" id="description" rows="3"></textarea>
    </div>
    <button class="mt-5 btn btn-primary" type="submit">Publish</button>
</form>
<%- include('partials/footer') -%>